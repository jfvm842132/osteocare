{
  "version": 2,
  "name": "osteocare-suporte-pos-tratamento",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": { "NODE_ENV": "production" },
  "cleanUrls": true
}